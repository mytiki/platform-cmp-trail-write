{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types", "jest"],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "Node",
    "allowJs": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "strict": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true
}
